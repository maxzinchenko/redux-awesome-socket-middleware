  "description": "This package makes web socket manage much easier with redux",
  "author": {
    "name": "Max Zinchenko",
    "email": "mx.zinchenko@gmail.com",
    "url": "https://github.com/maxzinchenko"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxzinchenko/redux-awesome-socket-middleware"
