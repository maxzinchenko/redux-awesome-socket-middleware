      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.13.14",
    "@rollup/plugin-replace": "2.3.4",
    "@rollup/plugin-typescript": "8.2.1",
    "@typescript-eslint/parser": "4.22.0",
    "cross-env": "7.0.2",
    "eslint": "7.10.0",
